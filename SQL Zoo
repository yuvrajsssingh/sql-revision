(1).
SELECT population FROM world
  WHERE name = 'Germany'

(2).
SELECT name, population FROM world
  WHERE name IN ('Sweden', 'Norway','Denmark');

(3).
SELECT name, area FROM world
  WHERE area BETWEEN 200000 AND 250000

(4).
select name from world where name like "y%"

(5).
SELECT name FROM world
  WHERE name LIKE '%y'

(6).
SELECT name FROM world
  WHERE name LIKE '%x%'

(7).
SELECT name FROM world
  WHERE name LIKE '%land'

(8).
SELECT name FROM world
  WHERE name LIKE 'c%' and name like "%ia"

(9).
SELECT name FROM world
  WHERE name LIKE '%oo%'

(10).
SELECT name
FROM world
WHERE name LIKE '%A%A%A%'
   OR name LIKE '%a%a%a%';

(11).
SELECT name FROM world
 WHERE name LIKE '_t%'
ORDER BY name

(12).
SELECT name FROM world
 WHERE name LIKE '%o__o%'

(13).
SELECT name
FROM world
WHERE LENGTH(name) = 4;

(14).
SELECT name
  FROM world
 WHERE name = capital

(15).
select name from world where capital=concat(name," City");

(16).
SELECT capital, name
FROM world
WHERE capital LIKE CONCAT('%', name, '%');

(17).
select capital , name from world
where CHAR_LENGTH(capital)>CHAR_LENGTH(name)
AND capital LIKE CONCAT(name, '%');

(18).
SELECT 
    name, 
     
    SUBSTRING(capital,LENGTH(name) + 1) AS extension
FROM 
    world
WHERE 
    LENGTH(capital) > LENGTH(name)
    AND capital LIKE CONCAT(name, '%')
    AND LENGTH(SUBSTRING(capital, LENGTH(name) + 1)) > 0;

(19).
SELECT name, continent, population FROM world

(20).
select name from world 
where population >=200000000

(21).
select name,(gdp/population) from world 
where population >=200000000

(22).
select name,(population/1000000) from world 
where continent='South America'

(23).
select name , population from world
where name in ("France","Germany","Italy")

(24).
select name from world 
where name like "united%"

(25).
select name , population , area from world
where area>=3000000 or population>=250000000

(26).
SELECT name, population, area
FROM world
WHERE 
    (area > 3000000 AND population <= 250000000) 
    OR 
    (population > 250000000 AND area <= 3000000) 

(27).
select name,	ROUND(population/1000000.0,2),ROUND(gdp/1000000000.0,2)
from world where continent='South America'

(28)
SELECT yr, subject, winner
  FROM nobel
 WHERE yr = 1950

(29)
SELECT winner
  FROM nobel
 WHERE yr = 1962
   AND subject = 'literature'

(30)
select yr,subject from nobel where winner="Albert Einstein"





